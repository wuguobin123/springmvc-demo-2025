# 生产环境变量配置文件示例
# 复制此文件为 .env 并填入实际值

# SiliconFlow AI API Key (必填)
SILICONFLOW_API_KEY=your-actual-api-key-here

# 数据库配置
DB_HOST=mysql
DB_PORT=3306
DB_NAME=springmvc_demo
DB_USERNAME=springmvc_user
DB_PASSWORD=your-secure-db-password

# MySQL Root密码
MYSQL_ROOT_PASSWORD=your-secure-root-password

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your-secure-redis-password

# RabbitMQ配置
RABBITMQ_HOST=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=your-secure-rabbitmq-password
RABBITMQ_VHOST=/prod

# 应用配置
SPRING_PROFILES_ACTIVE=prod
SERVER_PORT=8080

# AI配置
AI_BASE_URL=https://api.siliconflow.cn/v1
AI_MODEL=Qwen/QwQ-32B
AI_TEMPERATURE=0.7

# JPA配置
DDL_AUTO=update

# Redis数据库配置
REDIS_DATABASE=0
